{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "express",
            "image": "deanfost/file-uploader-express",
            "hostname": "express",
            "essential": "true", 
            "portMappings": [
                {
                    "containerPort": 3000
                }
            ]
        }, 
        {
            "name": "react", 
            "image": "deanfost/file-uploader-react",
            "hostname": "react",
            "essential": "true", 
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 5000
                }
            ]
        }
    ]
}